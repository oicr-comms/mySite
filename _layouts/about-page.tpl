---
layout: default
---

<div id="about-page">
    <div class="container">

        <div class="row">
            <div class="col-xs-12">
                <div class="title-style">
                    <h1>{{page.title_txt}}</h1>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-xs-12">
                <div class="about-description">
                    <p>{{page.description_txt | markdownify}}<p>
                    <div class="last-style lead">
                        <p><small>{{page.text_txt | markdownify}}</small><p>
                    </div>
                </div>
            </div>
        </div>


    </div>
</div>